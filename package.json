{
  "name": "dota-ihl-bot",
  "version": "0.0.0",
  "description": "A discord bot for hosting Dota 2 inhouse leagues",
  "main": "index.js",
  "scripts": {
    "docs:html": "jsdoc --configure .jsdoc.json",
    "docs:md": "jsdoc2md lib/*.js commands/**/*.js -r > api.md",
    "docs": "npm run docs:html && npm run docs:md",
    "db:drop": "sequelize --config config.js db:drop;",
    "db:create": "sequelize --config config.js db:create;",
    "db:migrate": "sequelize --config config.js db:migrate;",
    "db:seed:all": "sequelize --config config.js db:seed:all;",
    "db": "npm run db:create && npm run db:migrate && npm run db:seed:all",
    "test": "mocha",
    "start": "node index.js"
  },
  "author": "devilesk",
  "license": "ISC",
  "dependencies": {
    "bignumber.js": "^7.2.1",
    "bluebird": "^3.5.2",
    "discord.js": "^11.4.2",
    "discord.js-commando": "0.10.0",
    "dota2": "^6.1.1",
    "dotaconstants": "^6.1.1",
    "dotenv": "^6.0.0",
    "erlpack": "github:discordapp/erlpack",
    "express": "^4.16.3",
    "express-session": "^1.13.0",
    "human-readable-ids": "^1.0.4",
    "logform": "^1.9.1",
    "passport": "^0.4.0",
    "passport-discord": "^0.1.3",
    "pg": "^7.4.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.38.1",
    "sequelize-cli": "^4.1.1",
    "steam-id-convertor": "^1.0.1",
    "steamid": "^1.1.0",
    "steamidconvert": "^0.2.4",
    "uws": "^9.14.0",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.2",
    "zlib-sync": "^0.1.4"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "long": "^4.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "proxyquire": "^2.1.0",
    "sequelize-mocking": "^1.4.1",
    "sinon": "^6.2.0",
    "tui-jsdoc-template": "^1.2.2"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devilesk/dota-ihl-bot.git"
  },
  "bugs": {
    "url": "https://github.com/devilesk/dota-ihl-bot/issues"
  },
  "homepage": "https://github.com/devilesk/dota-ihl-bot#readme"
}
